@model RentMe.Core.Models.TenantViewModel

@{
    ViewData["Title"] = "Tenant Info";
}

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Expenses
        </dt>
        <dd class = "col-sm-10">
           @Html.ActionLink("DETAILS", "Expenses", "Expense", Model, null)        
        </dd>
        <dt class = "col-sm-2">
            First Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class = "col-sm-2">
            Last Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Phone)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Phone)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("REMOVE TENANT", "RemoveTenant", "Tenant", Model)
</div>
